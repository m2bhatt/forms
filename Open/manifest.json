{
  "manifest_version": 2,

  "name": "Open Tabs",
  "description": "Open important tabs for work",
  "version": "1.0",

  "icons": {
    "128": "icon.png"
  },

  "background" : {
    "scripts" : ["jquery.min.js", "background.js"]
  },

  "browser_action": {
    "default_title": "Open tabs",
    "default_icon": "icon.png"
  },

 "commands": {
    "_execute_page_action": {
    "suggested_key": {
    "default": "Alt+O",
    "windows": "Alt+O",
    "mac": "Alt+O"
            }
        }
    },

  "permissions": ["tabs", "activeTab"]
}
