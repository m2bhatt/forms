{
  "manifest_version": 2,

  "name": "App Stats",
  "description": "App Stats",
  "version": "3.2",

  "icons": {
    "128": "icon.png"
  },

  "background" : {
    "scripts" : ["jquery.min.js", "background.js"]
  },

  "page_action": {
    "default_title": "App Stats",
    "default_icon": "icon.png"
  },

 "commands": {
    "_execute_page_action": {
    "suggested_key": {
    "default": "Alt+S",
    "windows": "Alt+S",
    "mac": "Alt+S"
            }
        }
    },

  "permissions": ["tabs", "activeTab"]
}
